{
    "version": "1.0.0",
    "resource": "TestFiles/**/*",
    "dependencies": {
        "Microsoft.AspNet.Razor": "4.0.0-beta8",
        "Microsoft.AspNet.Razor.Test.Sources": {
            "version": "4.0.0-beta8",
            "type": "build"
        },
        "Microsoft.AspNet.Testing": "1.0.0-beta8",
        "xunit.runner.aspnet": "2.0.0-aspnet-beta8"
    },
    "commands": {
        "test": "xunit.runner.aspnet"
    },
    "frameworks": {
        "dnx451": {
            "dependencies": {
                "Moq": "4.2.1312.1622"
            }
        },
        "dnxcore50": {
            "dependencies": {
                "System.Diagnostics.TraceSource": "4.0.0-beta-23409",
                "System.Reflection.TypeExtensions": "4.0.1-beta-23409",
